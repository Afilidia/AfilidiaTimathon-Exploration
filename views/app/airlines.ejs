<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="icon" href="/assets/icons/world-purple.png">

    <!-- STYLES -->
    <link rel="stylesheet"  type="text/css" href="/stylesheets/menu.css">
    <link rel="stylesheet"  type="text/css" href="/stylesheets/index.css">
    <link rel="stylesheet"  type="text/css" href="/stylesheets/footer.css">
    <link rel="stylesheet"  type="text/css" href="/stylesheets/airlines.css">

    <!-- GOOGLE FONTS -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Fredoka+One&family=Source+Sans+Pro:ital,wght@0,200;0,300;0,400;0,600;0,700;0,900;1,300;1,600&display=swap" rel="stylesheet">

    <!-- JQUERY -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>

    <title>Airline finder | Planexplore</title>
</head>
<body>
    <!-- MENU IS BEING RENDERED BY render.js - (MenuComponent) -->

    <div class="container">
        <div class="result-wrapper">
            <div class="header">
                <span class="span-header">Airlines</span>
            </div>

            <div class="airlines scroll-container">

                <div class="airline">
                    <div class="airline-row">
                        <span class="callsign">IOZZM</span>
                        <div class="airports">
                            <span class="from-airport airport_iata">WAW</span>
                                <img src="/assets/icons/right-arrow.png" class="arrow-right">
                            <span class="to-airport airport_iata">KRK</span>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="settings-wrapper">
            <div class="header">
                <span class="span-header">Options</span>
            </div>

            <div class="options">
                <div class="option">
                    <span class="option-name">Airports</span>

                    <div class="wrapper">

                        <div class="column">
                            <div class="iata-iata">
                                <select class="iata select from">
                                    <option class="option" value="KRK">KRK</option>
                                </select>

                                <img src="/assets/icons/right-arrow.png" class="arrow-right">

                                <select class="iata select to">
                                    <option class="option" value="SVQ">SVQ</option>
                                </select>
                            </div>

                            <div class="name-name">
                                <span class="airport-name name-from">Krakow Airport</span>
                                <img src="/assets/icons/right-arrow.png" class="small-arrow arrow-right small-opacity-arrow">
                                <span class="airport-name name-to">Seville Airport</span>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- FOOTER IS BEING RENDERED BY render.js - (FooterComponent)-->

    <script src="/js/debugger.js" type="text/javascript"></script>
    <script src="/js/data.js" type="text/javascript"></script>
    <script src="/js/airlines.js" type="text/javascript"></script>
    <script src="/js/component.js" type="text/javascript"></script>
    <script src="/js/components/MenuComponent.js" type="text/javascript"></script>
    <script src="/js/components/FooterComponent.js" type="text/javascript"></script>
    <script src="/js/renderer.js" type="text/javascript"></script>
    <script src="/js/animations.js" type="text/javascript"></script>
    <script src="/js/setuper.js" type="text/javascript"></script>
</body>
</html>